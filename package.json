{
  "name": "frontend-starter",
  "version": "0.0.1",
  "scripts": {
    "client:dev": "vite",
    "client:build": "vue-tsc -p client/tsconfig.json --noEmit && vite build",
    "server:dev": "npm run server:build && concurrently \"tsc -p server/tsconfig.json -w\" \"nodemon -r source-map-support/register dist-server/api/index.js\"",
    "server:build": "tsc -p server/tsconfig.json",
    "server:start": "npm run server:build && node -r source-map-support/register dist-server/api/index.js",

    "build": "npm run client:build && npm run server:build",

    "lint": "eslint . --ext .ts && prettier . --check",
    "format": "eslint . --ext .ts --fix && prettier . --write"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  },
  "dependencies": {
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^5.7.0",
    "morgan": "^1.10.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 200,
    "endOfLine": "auto"
  }
}
